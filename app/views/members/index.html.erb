<%- model_class = Member -%>
<div class="page-header">
  <h1>Integrantes</h1>
</div>
<table class="table table-hover table-bordered table-condensed">
  <thead>
    <tr>
      <th>id</th>
      <th>Equipo</th>
      <th>Nombre</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
     <%= render @members %>
  </tbody>
  <tfoot>
    <tr>
      <td><%= link_to 'Nuevo Integrante', new_team_member_path(@team), :remote => true, :class => 'btn btn-success' %>
          <%= link_to 'Atras', teams_path, :class => 'btn btn-success' %>

      </td>
    </tr>
  </tfoot>
</table>
