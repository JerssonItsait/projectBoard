<%- model_class = Result -%>
<div class="page-header">
  <h1>Resultados</h1>
</div>
<table class="table table-hover table-bordered table-condensed">
  <thead>
    <tr>
      <th>id</th>
      <th>Respuesta</th>
      <th>Pregunta</th>
      <th>Equipo</th>
      <th>Ok</th>
    </tr>
  </thead>
  <tbody>
     <%= render @results %>
  </tbody>
  <tfoot>
    <tr>
      <td><%= link_to 'Nuevo Resultado', new_result_path, :remote => true, :class => 'btn btn-success' %></td>
    </tr>
  </tfoot>
</table>
